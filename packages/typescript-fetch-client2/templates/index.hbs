{{>header}}

{{>hooks/indexImports}}
import { Configuration } from './configuration{{#if @root.esm}}.js{{/if}}'
{{#each groups}}
import { withConfiguration as {{identifier name}}ApiWithConfiguration } from './api/{{identifier name}}{{#if @root.esm}}.js{{/if}}'
{{/each}}

export * from "./models{{#if @root.esm}}.js{{/if}}";
export * from "./configuration{{#if @root.esm}}.js{{/if}}";
export { RequiredError, isUnexpectedResponse, isDocumentedResponse } from "./runtime{{#if @root.esm}}.js{{/if}}";
export type { FetchAPI, FetchArgs, UndocumentedResponse, FetchErrorResponse, UnexpectedResponse } from "./runtime{{#if @root.esm}}.js{{/if}}";

{{#each groups}}
export { default as {{className name}}Api } from './api/{{identifier name}}{{#if @root.esm}}.js{{/if}}'
{{/each}}

export function withConfiguration(configuration: Configuration) {
	return {
		{{#each groups}}
		{{className name}}Api: {{identifier name}}ApiWithConfiguration(configuration),
		{{/each}}
	}
}