{{>header}}

export namespace Api {
{{#each models}}
	{{#if (isEnum)}}
	{{>modelEnum}}
	{{else}}
	{{>modelGeneric}}
	{{/if}}

{{/each}}
}

{{#each models}}
{{#if (isEnum)}}
/**
 * {{{description}}}
 * @export
 * @enum {string}
 */
export import {{className name}} = Api.{{className name}}
{{else}}
/**
 * {{{description}}}
 * @export
 * @interface {{className name}}
 */
export type {{className name}} = Api.{{className name}}
{{/if}}

{{/each}}
